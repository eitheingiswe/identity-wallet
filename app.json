{
  "expo": {
    "name": "SGFlight",
    "slug": "SGFlight",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "sgflight",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "sg.gov.caas.SGFlight",
      "buildNumber": "1.0.6",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires to access your camera to scan QRcode"
      }
    },
    "android": {
      "package": "sg.gov.caas.SGFlight",
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#171717"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    }
  }
}
